			          <li><%= link_to 'Perfil', perfils_path %></li>
			          <li><%= link_to 'Usuario', usuarios_path %></li>
			          <li><a href="javascript:void(0)">Permissão</a></li>
		        	</ul>
		      	</li>
		      	<li><a href="javascript:void(0)">Sair</a></li>
		    </ul>  
  		</div>
		<%= yield %>
		<div id="rodape">
    		Sistema adquirido com recursos do PNAFM
    		<span class="versao"> V 2.1 </span>
  		</div>
	</div>
	<%= javascript_include_tag 'jquery-1.3.2.min.js' %>
	<%= javascript_include_tag 'jquery.meio.mask.js' %>
	<%= javascript_include_tag 'jquery-ui-1.7.2.custom.min.js' %>
	<%= javascript_include_tag 'jquery.validate.js' %>
	<%= javascript_include_tag 'funcoes.js' %>
